{
  "short_name": "Cleansumer",
  "name": "Cleansumer Extension",
  "version": "0.1",
  "manifest_version": 3,
  "description": "First iteration of a browser extension tool that checks for an aggregation of corporate ethics metrics for a company",
  "background": {
    "service_worker": "./background.js"
    },
  "permissions": [ "storage"],
  "author": "Robert Chung & Purnima Vijaya",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Cleansumer",
    "default_icon": {
      "16": "/images/react-logo-16.png",
      "32": "/images/react-logo-32.png",
      "48": "/images/react-logo-48.png",
      "128": "/images/react-logo-128.png"
      }
    },
    "icons": {
      "16": "/images/react-logo-16.png",
      "32": "/images/react-logo-32.png",
      "48": "/images/react-logo-48.png",
      "128": "/images/react-logo-128.png"
      }
}
