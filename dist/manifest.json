{
    "name": "Cleansumer",
    "version": "0.0.2",
    "manifest_version": 3,
    "description": "Rotten Tomatoes for Company Ethics",
    "action": {
        "default_title": "Cleansumer",
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/temp.png",
            "48": "icons/temp.png",
            "128": "icons/temp.png"
          }
    },
    "icons": {
        "16": "icons/temp.png",
        "48": "icons/temp.png",
        "128": "icons/temp.png"
    },
    "default_locale": "en",
    "background": {
        "service_worker": "js/background.js"
      },
    "host_permissions": [
        "https://bizdataapi.azurewebsites.net/*",
        "https://*/*"
    ],
    "externally_connectable": {
        "matches": [ "https://bizdataapi.azurewebsites.net/*" ]
    },
    "permissions": [
        "storage"
    ],
    "content_security_policy": {
        "script-src": "self",
        "object-src": "self"
      },
    "content_scripts": [
        {
            "matches": [
                "https://*/*"
            ],
            "js": [
                "js/content.js"
            ]
        }
    ]
}